pipeline{
    agent any
    stages{
        stage("echo a line"){
            steps{
                echo 'this is step one'
            }
            
        }
        stage("run a command") {
    steps {
        bat 'cd'
    }
}

stage('Run Multiple command') {
            steps {
                bat '''
                dir
                date /t
                cd
                '''
            }
        }
 stage('Calendar') {
            steps {
                powershell(script: './scripts/calendar.ps1')
            }
        }
}
}
