pipeline{
    agent any
    stages{
        stage("Print line"){
            steps{
                echo 'this is step one'
            }
            
        }
        stage("Run single command") {
    steps {
        bat 'cd'
    }
}

stage('Run Multiple command') {
            steps {
                bat '''
                dir
                date /t
                cd
                '''
            }
        }
}
}
